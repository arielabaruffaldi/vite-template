{
  "name": "hci-k3-app",
  "private": true,
  "version": "0.0.0",
  "description": "HCI K3 App",
  "author": "Health Care Informed",
  "type": "module",
  "scripts": {
    "dev": "chmod +x ./env.sh && ./env.sh && cp env-config.js ./public/ && vite",
    "start-scss-typing": "npx typed-scss-modules src --watch --nameFormat none --ignore \"**/*.extensions.scss\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(json|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-types": "tsc --project tsconfig.json --pretty --noEmit",
    "check-format": "npm run prettier -- --list-different",
    "preview": "vite preview",
    "lingui:extract": "lingui extract",
    "lingui:compile": "lingui compile",
    "lingui:all": "npm run lingui:extract && npm run lingui:compile",
    "convert:img": "./convert.sh"
  },
  "dependencies": {
    "@lingui/react": "^4.7.1",
    "axios": "^1.6.7",
    "classnames": "^2.5.1",
    "node-sass": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@lingui/cli": "^4.7.1",
    "@lingui/core": "^4.7.1",
    "@lingui/macro": "^4.7.1",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "make-plural": "^7.3.0",
    "prettier": "3.2.5",
    "sass": "^1.71.1",
    "typed-scss-modules": "^8.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.1"
  }
}
